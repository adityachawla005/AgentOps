generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Session {
  id         Int      @id @default(autoincrement())
  session_id String
  page       String
  events     Json
  createdAt  DateTime @default(now())
}

model Variant {
  id          Int      @id @default(autoincrement())
  name        String
  elementId   String
  version     Int
  impressions Int      @default(0)
  clicks      Int      @default(0)
  createdAt   DateTime @default(now())
}
